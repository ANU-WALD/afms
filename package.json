{
  "name": "fmc",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 9001 --aot=false",
    "remote": "ng serve --port 9001 --host 0.0.0.0 --aot false --proxy-config proxy.conf.json",
    "build": "if [ $BRANCH = 'production' ]; then export PREFIX='/afms/'; elif [ $BRANCH = 'defence' ]; then export PREFIX='/afms/defence/'; else export PREFIX='/afms/staging/'; fi; npm run build-with-prefix",
    "build-with-prefix": "echo prefix=$PREFIX && ng build --prod --no-aot --base-href $PREFIX",
    "deprecated-build": "npm run transpile-plotly && npm run fix-plotly",
    "transpile-plotly": "mkdir -p plotly-transpiled && browserify node_modules/typedarray/index.js -o plotly-transpiled/typedarray.js && cd node_modules/plotly.js/dist && babel -d ../../../plotly-transpiled plotly.js plotly.min.js",
    "fix-plotly": "cp plotly-transpiled/plotly.min.js dist/plotly.js && cp plotly-transpiled/typedaray.js dist && sed 's-../plotly-transpiled-.-g' < dist/index.html > tmp && mv tmp dist/index.html",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.7",
    "@angular/animations": "^8.2.2",
    "@angular/common": "^8.2.2",
    "@angular/compiler": "^8.2.2",
    "@angular/compiler-cli": "^8.2.2",
    "@angular/core": "^8.2.2",
    "@angular/forms": "^8.2.2",
    "@angular/platform-browser": "^8.2.2",
    "@angular/platform-browser-dynamic": "^8.2.2",
    "@angular/platform-server": "^8.2.2",
    "@angular/router": "^8.2.2",
    "@ng-bootstrap/ng-bootstrap": "^4.0.3",
    "@types/googlemaps": "^3.26.7",
    "@types/proj4": "^2.3.4",
    "bootstrap": "^4.5.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "dap-query-js": "github:flowmatters/dap-query-js",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "map-wald": "github:ANU-WALD/map-wald",
    "map-wald-visual": "github:ANU-WALD/map-wald-visual",
    "ngx-pipes": "1.6.5",
    "plotly.js": "git+https://github.com/arlowhite/plotly.js.git#no-polyfill",
    "proj4": "^2.3.17",
    "rxjs": "^6.5.3",
    "store": "^2.0.12",
    "typescript": "~3.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.29",
    "@angular/cli": "^8.2.2",
    "@types/file-saver": "0.0.1",
    "@types/geojson": "^1.0.3",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "@types/proj4": "^2.3.4",
    "@types/storejs": "^2.0.3",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^7.0.0",
    "ts-node": "~2.0.0",
    "tslint": "^5.20.1"
  }
}
